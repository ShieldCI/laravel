{
    "name": "shieldci/laravel",
    "description": "ShieldCI Laravel Package - Security and code quality analysis for Laravel applications",
    "keywords": [
        "laravel",
        "security",
        "static-analysis",
        "code-quality",
        "vulnerability-scanner",
        "shieldci"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ShieldCI",
            "email": "support@shieldci.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "shieldci/analyzers-core": "^1.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "ShieldCI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShieldCI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShieldCI\\ShieldCIServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
